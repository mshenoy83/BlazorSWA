{
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "routes": [
        {
            "route": "api/.*",
            "methods": ["GET", "POST"],
            "serve": "/api/RedirectionTrigger"
        },
        {
            "route": "/saml/.*",
            "methods": ["GET", "POST"],
            "serve": "/api/RedirectionTrigger"
        
        }
    ],
    "platform": {
        "apiRuntime": "dotnet-isolated:8.0"
    }
}